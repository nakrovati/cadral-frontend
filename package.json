{
  "name": "cadral-frontend",
  "version": "1.0.0",
  "description": "The frontend of the Cadral url shortening service.",
  "main": "./src/main.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nakrovati/cadral-frontend.git"
  },
  "author": "Daniil Zhuravlev",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/nakrovati/cadral-frontend/issues"
  },
  "homepage": "https://github.com/nakrovati/cadral-frontend#readme",
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --ext .js,.vue src --fix",
    "style": "stylelint **/*.{css,scss,vue}",
    "style:fix": "stylelint **/*.{css,scss,vue} --fix",
    "format": "prettier . --write",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{css,scss,vue}": "stylelint --fix",
    "*.{json,md,html}": "prettier --write",
    "*.{js,vue}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "vue": "^3.2.36",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.0",
    "autoprefixer": "^10.4.5",
    "c8": "^7.11.3",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.2.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "postcss": "^8.4.12",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.0",
    "sass": "^1.51.0",
    "stylelint": "^14.8.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-order": "^5.0.0",
    "vite": "^3.0.0",
    "vite-svg-loader": "^3.3.0",
    "vitest": "^0.19.1"
  }
}
