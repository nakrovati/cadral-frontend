{
  "compilerOptions": {
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "Fonts/*": ["src/assets/fonts/*"],
      "Icons/*": ["src/assets/icons/*"],
      "Images/*": ["src/assets/images/*"],
      "Components/*": ["src/components/*"],
      "Services/*": ["src/services/*"],
      "Styles/*": ["src/styles/*"]
    }
  },
  "include": ["src", "public"]
}
